                          </characteristics>
                        </profile>
                        <profile id="af96-dc22-3cfc-b1fe" name="Booming Roar" book="Chaos Battletoam: Beasts of Chaos" page="68" hidden="false" profileTypeId="f71f-b0a4-730e-ced3" profileTypeName="Command Abilities">
